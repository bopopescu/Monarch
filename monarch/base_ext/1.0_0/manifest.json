{
  "manifest_version": 2,
  "name": "%APP_NAME%",
  "version": "1",
  "minimum_chrome_version": "23",
  "app": {
     "background": {
        "scripts": ["background.js"]
    }
  },
  "icons": { "16": "img/icon16.png",
           "48": "img/icon48.png",
          "128": "img/icon128.png" },
  "permissions": [
      "browser",
      "webview"
  ],
  "dynamic": true,
  "dynamic_url": "%APP_URL%"
}